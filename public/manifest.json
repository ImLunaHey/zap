{
  "id": "https://zap.fish.lgbt/manifest.json",
  "name": "Zap ⚡️",
  "short_name": "Zap ⚡️",
  "start_url": "/",
  "display": "standalone",
  "theme_color": "#f1d800",
  "background_color": "#f1d800",
  "description": "Zap your friends ⚡️",
  "icons": [
    {
      "src": "/icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+zap",
      "url": "/zap?id=%s"
    }
  ]
}
